{
  "extends": "../../tsconfig.react.json",
  "include": [
    ".react-router/types/**/*",
    "app/**/*",
    "app/**/.server/**/*",
    "app/**/.client/**/*",
    "server/**/*"
  ],
  "references": [
    {
      "path": "../../core"
    }
  ],
  "compilerOptions": {
    "types": [
      "vite/client"
    ],
    "rootDirs": [
      ".",
      "./.react-router/types"
    ],
    "paths": {
      "~/*": [
        "./app/*"
      ],
      "~/server*": [
        "./app/*"
      ],
    },
  }
}